{
    "ver": "0.0.1",
    "authors": [
        "psdehal"
    ],
    "contact": "",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [
        {
            "id": "input_assembly_upa",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseGenomes.ContigSet","KBaseGenomeAnnotations.Assembly" ]
            }
        },
        {
            "id": "search_db",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "Ecoli" ],
            "field_type": "dropdown",
            "dropdown_options":{
            "options": [{
                "value": "Ecoli",
                "display": "Ecoli"
              },
              {
                "value": "Genbank",
                "display": "Genbank"
              },
              {
                "value": "img_bact_isol",
                "display": "IMG Bacterial Isolates"
              },
              {
                "value": "img_bact_mags",
                "display": "IMG Bacterial MAGs"
              },
              {
                "value": "img_bact_sags",
                "display": "IMG Bacterial SAGs"
              },
              {
                "value": "img_arch_isol",
                "display": "IMG Archael Isolates"
              },
              {
                "value": "img_arch_mags",
                "display": "IMG Archael MAGs"
              },
              {
                "value": "img_arch_sags",
                "display": "IMG Archael SAGs"
              },
              {
                "value": "img_metag_metat_no_raw",
                "display": "IMG Metagenomes & Metatranscriptomes, no raw reads"
              },
              {
                "value": "kb_refseq_ci_1000",
                "display": "KBase CI RefSeq first 1000 assemblies"
              }
            ]
          }
        },
        {
            "id": "scaled",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "1000" ],
            "field_type": "text",
            "text_options": {
              "validate_as": "int"
            }
        },
        {
            "id": "containment",
            "optional": true,
            "advance": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_sourmash",
            "method": "run_sourmash_search",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "input_assembly_upa",
                    "target_property": "input_assembly_upa",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "search_db",
                    "target_property": "search_db"
                },
                {
                    "input_parameter": "scaled",
                    "target_property": "scaled"
                },
                {
                    "input_parameter": "containment",
                    "target_property": "containment"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspaceName"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
